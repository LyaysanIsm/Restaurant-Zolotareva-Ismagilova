@model RestaurantWebSupplier.Models.RegistrationModel
@{
    ViewData["Title"] = "Registration";
}
<h1>@ViewData["Title"]</h1>

@using (Html.BeginForm("Registration", "Supplier"))
{
    <p>ФИО: @Html.TextBoxFor(x => x.SupplierFIO)</p>
    <p>Пароль: @Html.TextBoxFor(x => x.Password)</p>
    <p>E-Mail: @Html.TextBoxFor(x => x.Email)</p>
    @Html.ValidationSummary()
    <input type="submit" value="Sign up" />
}